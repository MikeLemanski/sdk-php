<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <parser>
        <target>temp/output</target>
    </parser>
    <logging>
        <level>warn</level>
    </logging>
    <transformer>
        <target>./doc/generated</target>
    </transformer>
    <transformations>
        <!--<template name="clean"/>-->
        <template name="doc/doc-template" />
    </transformations>
    <files>
        <directory>./lib</directory>
        <ignore>Proto/*</ignore>
    </files>
</phpdoc>